<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>/index.do</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>init</servlet-name>
    <servlet-class>cc.test.Controller.InitServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>init</servlet-name>
    <url-pattern>/index.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>shopping</servlet-name>
    <servlet-class>cc.test.Controller.ShopingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>shopping</servlet-name>
    <url-pattern>/shopping.view</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>cart</servlet-name>
    <servlet-class>cc.test.Controller.CarServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>cart</servlet-name>
    <url-pattern>/cart.jsp</url-pattern>
  </servlet-mapping>
</web-app>
